{
  "name": "automart",
  "version": "1.0.0",
  "description": "A car sale web application for selling and buying new and used cars",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node index.js",
    "start": "babel-node index.js",
    "test": " nyc --reporter=html --reporter=text mocha --require @babel/register --exit || true",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls", 
    "generate-lcov": "nyc report --reporter=text-lcov >lcov.info", 
    "coveralls-coverage": "coveralls < lcov.info", 
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info", 
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleave3/AutoMart.git"
  },
  "keywords": [
    "car",
    "sale",
    "used",
    "new",
    "affordable",
    "cars"
  ],
  "author": "owhiroro cleave <owhiroroeghele@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cleave3/AutoMart/issues"
  },
  "homepage": "https://github.com/cleave3/AutoMart#readme",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@hapi/joi": "^15.0.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  }
}
